<% @locations.each do |location| %>
    <h3><%= link_to location.name, location %>
    <h3><%= location.city%>
    <%= link_to "Edit", edit_location_path(location) %>
    <%= link_to "Delete", location_path(location), method: :delete, class: "destroy", data: {confirm: "Are you sure?"} %></h3>
    
    
<%end%>
